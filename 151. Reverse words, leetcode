class Solution {
    // Function to reverse words in a given string
    public String reverseWords(String s) {
        // Code here
        String[] result=s.split(" +");
        StringBuilder sb=new StringBuilder();
        for(int i=result.length-1;i>=0;i--)
        {
            sb.append(result[i]);
            sb.append(" ");
            
        }
        return sb.toString().trim();
        
    }
}
